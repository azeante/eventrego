<p id="notice"><%= notice %></p>

<h1>Presenters</h1>

<table>
  <thead>
    <tr>
      <th>Biography</th>
      <th>Remunerationarrangements</th>
      <th>Allowance</th>
      <th>Remuneration</th>
      <th>Person</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @presenters.each do |presenter| %>
      <tr>
        <td><%= presenter.biography %></td>
        <td><%= presenter.remunerationArrangements %></td>
        <td><%= presenter.allowance %></td>
        <td><%= presenter.remuneration %></td>
        <td><%= presenter.person %></td>
        <td><%= presenter.event %></td>
        <td><%= link_to 'Show', presenter %></td>
        <td><%= link_to 'Edit', edit_presenter_path(presenter) %></td>
        <td><%= link_to 'Destroy', presenter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Presenter', new_presenter_path %>
